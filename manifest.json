{
	"name": "Auto Login Unitn",
	"version": "1",
	"description": "Skip intermediate login pages",
	"manifest_version": 3,
	"content_scripts": [
		{
			"matches": [
				"https://idp.unitn.it/*",
				"https://idp.unitn.it/idp/*",
				"https://webapps.unitn.it/gestionecorsi/",
				"https://didatticaonline.unitn.it/dol/*"
			],
			"js": [
				"main.js"
			]
		}
	],
	"icons": {
		"16": "/icons/icon-16.png",
		"32": "/icons/icon-32.png",
		"48": "/icons/icon-48.png",
		"128": "/icons/icon-128.png"
	}
}